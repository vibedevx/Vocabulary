{
  "manifest_version": 3,
  "name": "Vocabulary Collector",
  "version": "1.0",
  "description": "Collect selected text into a vocabulary list.",
  "permissions": ["contextMenus", "storage", "tts", "sidePanel"],
  "host_permissions": ["https://api.dictionaryapi.dev/*"],
  "background": {
    "service_worker": "background.js"
  },
  "side_panel": {
    "default_path": "sidepanel.html"
  },
  "action": {
    "default_title": "Vocabulary"
  }
}
